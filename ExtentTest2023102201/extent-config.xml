<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>standard</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle> UI Test</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>UI Function Test Report
			<!-- 
         <![CDATA[
                <img src='http://127.0.0.1:18080/jenkins/job/ExtentDemo1/ws/logo.svg'/>
            ]]> -->
		</reportName>

		<enableCategoryView>true</enableCategoryView>
		<enableAuthorView>false</enableAuthorView>
		<enableExceptionView>true</enableExceptionView>
		<enableTestRunnerLogsView>true</enableTestRunnerLogsView>


		<!-- location of charts in the test view -->
		<!-- top, bottom -->
		<testViewChartLocation>bottom</testViewChartLocation>
		
		<!-- global date format override -->
		<!-- defaults to yyyy-MM-dd -->
		<dateFormat>yyyy-MM-dd</dateFormat>
		
		<!-- global time format override -->
		<!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>

		<!-- custom javascript -->
		<scripts>
            <![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
            <![CDATA[
            .report-name { padding-left: 18px; } .report-name > img { float: 
      left;height: 90%;margin-left: 30px;margin-top: 3px;width: auto; }
        ]]>
		</styles>
	</configuration>
</extentreports>